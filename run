#!/bin/bash

set -e
echo "---> run script starting $pwd "
ls -la

#cd $HOME

echo "====> sh export.sh -opts=${OPTIONS} -ou=${OLD_OPENSHIFT_USERNAME} -opp=${OLD_OPENSHIFT_PASSWORD} -ourl=${OLD_OPENSHIFT_URL} -op=${OLD_OPENSHIFT_PROJECT} -nu=${NEW_OPENSHIFT_USERNAME} -npp=${NEW_OPENSHIFT_PASSWORD} -nurl=${NEW_OPENSHIFT_URL} -np=${NEW_OPENSHIFT_PROJECT}"


/bin/sh export.sh -opts=${OPTIONS} -ou=${OLD_OPENSHIFT_USERNAME} -opp=${OLD_OPENSHIFT_PASSWORD} -ourl=${OLD_OPENSHIFT_URL} -op=${OLD_OPENSHIFT_PROJECT} -nu=${NEW_OPENSHIFT_USERNAME} -npp=${NEW_OPENSHIFT_PASSWORD} -nurl=${NEW_OPENSHIFT_URL} -np=${NEW_OPENSHIFT_PROJECT}
